{
	"name": "musik-player",
	"description": "Linux - NodeJS: `sudo apt install nodejs` - MPlayer: `sudo apt install mplayer` - NPM `sudo apt install npm`",
	"version": "1.1.0",
	"author": "LFF5644",
	"license": "ISC",
	"main": "main.js",
	"scripts": {
		"install": "sudo apt install screen mplayer",
		"start": "screen -S musikPlayer node main.js",
		"start-hidden": "screen -dmS musikPlayer node main.js",
		"show-console": "screen -dr musikPlayer",
		"stop": "screen -X -S musikPlayer quit",
		"update": "git pull; npm update"
	},
	"dependencies": {
		"audio-player-lib": "git+ssh://git@github.com:LFF5644/audioPlayerLib.git",
		"socket-tcp": "git+ssh://git@github.com:LFF5644/socketTCP.git",
		"directory-tools": "git+ssh://git@github.com:LFF5644/directory-tools.git"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LFF5644/MusikPlayer.git"
	},
	"bugs": {
		"url": "https://github.com/LFF5644/MusikPlayer/issues"
	},
	"homepage": "https://github.com/LFF5644/MusikPlayer#readme"
}
